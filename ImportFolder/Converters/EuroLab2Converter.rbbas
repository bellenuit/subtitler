#tag ClassProtected Class EuroLab2ConverterInherits cTitlelistConverter	#tag Event		Sub DoConvert(s as string)		  dim i,j,countlines,item as integer		  dim start, ende, tctext as string		  dim tit as ctitle		  dim separator as string		  dim word, words(-1), lines(-1), line, titles(-1), title, substring as string		  dim state as string		  dim tab as string		  dim d as dictionary		  dim pos,pos2, hours as integer		  dim bi as boolean		  dim titleitalic as boolean		  dim eventnrfield as string		  dim titletext as string		  dim feetframes, feetframesin, feetframesout, feetin, framein, feetout, frameout as string		  dim mytc, offsettimecode as timecode		  dim emptyline as boolean		  		  		  		  		  offsettimecode = new timecode		  offsettimecode.SetFps(tc.Fps)		  		  separator = guessseparator(s)		  tab = chr(9)		  		  titles = split(s,separator+"@"+separator)		  countlines = ubound(titles)		  		  pos = 1		  for i = 0 to ubound(titles) 		    		    lines = split(titles(i),separator)		    // test for header		    		    mytc = new TimeCode		    mytc.SetFps(tc.Fps)		    		    tit = new ctitle		    		    for j=0 to ubound(lines)		      line = lines(j)		      line = replaceall(line,tab,"") // remove tabs		      		      if left(line,1) = "*" then		        // *    00405.01-00409.11 00.00 11.0 1 0000 00 16-090-090		        line = mid(line,2) // remove *		        		        feetframes = NthField(line," ",1)		        feetframesin= NthField(feetframes,"-",1)		        feetframesout= NthField(feetframes,"-",2)		        		        mytc.SetFeet(val(feetframesin))		        mytc.Plus(offsettimecode)		        tit.start = mytc.GetText		        		        mytc.SetFeet(val(feetframesout))		        mytc.Plus(offsettimecode)		        tit.ende = mytc.GetText		      elseif line="" then		        // ignore		      elseif left(line,5) = "REEL " then		        hours = val(mid(line,6))		        offsettimecode.SetTotalSeconds hours*60*60		      else // text		        pos2 = instr(pos2,s,line)		        titletext = makeitalic(line,pos2)		        tit.lines.append titletext		      end		    next		    		    titlelist.titles.append tit		    		    if i mod 10 = 0 then		      progress 33+66*i/ubound(titles) 		    end		    if userCancelled then		      exit		    end		    		  next		  		  createedl uppercase(documentname),0,ubound(titlelist.titles) 		  		End Sub	#tag EndEvent	#tag Method, Flags = &h0		Sub Constructor()		  label = "EuroLab 2"		End Sub	#tag EndMethod	#tag Note, Name = sample				REEL 1		*    00361.14-00366.14 00.00 11.0 1 0000 00 16-090-090		Jag drömde så konstigt i natt.		@		*    00368.02-00373.00 00.00 11.0 1 0000 00 16-090-090		Jag dansade den vita svanen.		@		*    00373.09-00380.00 00.00 11.0 1 0000 00 16-090-090		Fast i en annan koreografi.		Mer som Bolsjojs.		@		*    00384.06-00392.13 00.00 11.0 1 0000 00 16-090-090		Det var prologen,		där Rothbart förtrollar Odette.		@		*    00405.01-00409.11 00.00 11.0 1 0000 00 16-090-090		Titta, så rosa! Läckert.		@		*    00411.01-00414.03 00.00 11.0 1 0000 00 16-090-090		Läckert!		@		*    00414.07-00421.06 00.00 11.0 1 0000 00 16-090-090		-Du är på bra humör i dag.		-Han har lovat mig större roller.		@		*    00421.10-00432.09 00.00 11.0 1 0000 00 16-090-090		Fattas bara. Du har varit där länge nog,		och du är mest ambitiös av alla.		@		*    00436.14-00440.01 00.00 11.0 1 0000 00 16-090-090		Upp.		@		*    00441.04-00446.04 00.00 11.0 1 0000 00 16-090-090		-Vad är det där?		-Vad då?		@		*    00450.04-00453.10 00.00 11.0 1 0000 00 16-090-090		Inget.		@		*    00457.13-00462.05 00.00 11.0 1 0000 00 16-090-090		Ska jag inte följa med dig?		@		*    00465.02-00469.03 00.00 11.0 1 0000 00 16-090-090		Min älskling.		@		*    00555.05-00561.09 00.00 11.0 1 0000 00 16-090-090		VINTERKLASSIKER		I NY TAPPNING		@		*    00577.06-00585.03 00.00 11.0 1 0000 00 16-090-090		-Obegripligt att Beth 