#tag ClassProtected Class RtfStylesheet	#tag Method, Flags = &h0		Sub Apply(p as RTFparser)		  dim i as integer		  for i = 0 to ubound(commands)		    p.docommand commands(i),parameters(i)		  next		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub Constructor()		  id = 222		  sbasedon = 222		  snext = -1		  redim commands(-1)		  redim parameters(-1)		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub Parse(input as ramStream)		  dim com as string		  dim par as integer		  dim bracket as integer		  dim car as string		  		  bracket = 1		  		  while not input.eof		    car = input.read(1)		    select case car		    case "{"		      bracket = bracket + 1		    case "}"		      bracket = bracket -1		    case "\"		      RTFparsecommand(input,com,par)		      select case com		      case "s"		        id = par		      case "sbasedon"		        sbasedon = par		      case "snext"		        sbasedon =par		      case "*"		        RtfIgnoreGroup(input)		        bracket = bracket -1		      else		        commands.append com		        parameters.append par		      end		    case " "		    case chr(13)		    case chr(10)		    else		      input.position(input.position-1)		      label = RTFParseLabel(input)		    end		    if bracket = 0 then		      exit		    end		  wend		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub Version()		  // 1.1 no changes		  // 1.0a2 9.11.00 no changes in this class		   // 1.0a1 14.10.00		End Sub	#tag EndMethod	#tag Property, Flags = &h0		commands(0) As string	#tag EndProperty	#tag Property, Flags = &h0		id As integer	#tag EndProperty	#tag Property, Flags = &h0		label As string	#tag EndProperty	#tag Property, Flags = &h0		parameters(0) As integer	#tag EndProperty	#tag Property, Flags = &h0		sbasedon As integer	#tag EndProperty	#tag Property, Flags = &h0		snext As integer	#tag EndProperty	#tag ViewBehavior		#tag ViewProperty			Name="id"			Group="Behavior"			InitialValue="0"			Type="integer"		#tag EndViewProperty		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue="-2147483648"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="label"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="sbasedon"			Group="Behavior"			InitialValue="0"			Type="integer"		#tag EndViewProperty		#tag ViewProperty			Name="snext"			Group="Behavior"			InitialValue="0"			Type="integer"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty	#tag EndViewBehaviorEnd Class#tag EndClass