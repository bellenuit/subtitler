#tag ClassProtected Class cEDLevent	#tag Method, Flags = &h0		Function asciiconvert(s as string) As string		  // is faster than textconverter, throw away non readable characters and replace by ?		  		  dim m as memoryBlock		  dim i,ub,interrogation as integer		  ub = lenb(s)		  m = newmemoryBlock(ub+1)		  m.stringValue(0,ub+1) = s		  		  interrogation = asc("?")		  		  for i = 0 to ub		    if m.byte(i) > 127 then		      m.byte(i) = interrogation		    end		  next		  		  return m.stringValue(0,ub+1)		  		End Function	#tag EndMethod	#tag Method, Flags = &h0		Sub Constructor()		  tracks = "V"		  transition = "C"		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function dump() As string		  dim line as string		  dim shortcomment as string		  		  line = format(eventnr mod 1000,"000")+"  "		  line = line + format(tape,"0000")+"     "		  line = line + left(tracks + "      ",6) 		  line = line + left(transition + "         ", 9)		  line = line + left(sourcein,11) + " " + left(sourceout,11) + " " 		  line = line + left(recordin,11) + " " + left(recordout,11) + newline		  		  // comments		  		  if comment <> "" then		    line = line + "* FROM CLIP NAME:  " + filename + ".TIF" + newline		    line = line + "* FROM CLIP IS A STILL" + newline		    		    '* FROM CLIP NAME:  0001.TIF		    '* COMMENT:		    '* FROM CLIP IS A STILL		    		  end		  		  return line		End Function	#tag EndMethod	#tag Property, Flags = &h0		comment As string	#tag EndProperty	#tag Property, Flags = &h0		eventnr As integer	#tag EndProperty	#tag Property, Flags = &h0		filename As string	#tag EndProperty	#tag Property, Flags = &h0		newline As string	#tag EndProperty	#tag Property, Flags = &h0		recordin As string	#tag EndProperty	#tag Property, Flags = &h0		recordout As string	#tag EndProperty	#tag Property, Flags = &h0		sourcein As string	#tag EndProperty	#tag Property, Flags = &h0		sourceout As string	#tag EndProperty	#tag Property, Flags = &h0		tape As integer	#tag EndProperty	#tag Property, Flags = &h0		tracks As string	#tag EndProperty	#tag Property, Flags = &h0		transition As string	#tag EndProperty	#tag ViewBehavior		#tag ViewProperty			Name="comment"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="eventnr"			Group="Behavior"			InitialValue="0"			Type="integer"		#tag EndViewProperty		#tag ViewProperty			Name="filename"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue="-2147483648"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="newline"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="recordin"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="recordout"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="sourcein"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="sourceout"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="tape"			Group="Behavior"			InitialValue="0"			Type="integer"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="tracks"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="transition"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty	#tag EndViewBehaviorEnd Class#tag EndClass