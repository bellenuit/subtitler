ring("picturebackgroundpict", s1) and _		    languagedict.getstring("picturebackgroundmovie", s2) then		    i = picturebackgroundMenu.listindex		    picturebackgroundMenu.init array(s0,s1,s2)		    picturebackgroundMenu.listindex = i		  end		  if languagedict.getstring("savetitle", value) then checksaveTitle.caption = value		  if languagedict.getstring("select", value) then pictureButton.caption = value		  if languagedict.getstring("monitor", value) then groupBox1.Caption = value		  if languagedict.getstring("preview", value) then groupBox5.caption = value		  if languagedict.getstring("editor", value) then groupBox4.caption = value		  if languagedict.getstring("preferenceslanguage", value) then groupBox6.caption = value		  		  if languagedict.getstring("preferencestextsize", value) then statictext5.text = value		  if languagedict.getstring("preferencestextsize", value) then statictext6.text = value		  if languagedict.getstring("preferencessplitdistance", value) then splitlabel.text = value		  		  'if languagedict.getstring("preferencesparsestyletags", value) then parsestyletagsoption.caption = value		  		  if languagedict.getstring("windoworiginal", value) then 		    monitorscalemenu.init array("25%","50%","100%",value)		    MonitorScaleMenu.listindex = app.monitorsize		  end		End Sub	#tag EndMethod	#tag Property, Flags = &h0		haschanged As boolean	#tag EndProperty	#tag Property, Flags = &h0		oldformat As string	#tag EndProperty#tag EndWindowCode#tag Events GroupBox1	#tag Event		Sub Open()		  setlanguage		End Sub	#tag EndEvent#tag EndEvents#tag Events CheckSaveTitle	#tag Event		Sub Open()		  me.value =app.showsavetitle		End Sub	#tag EndEvent	#tag Event		Sub Action()		  dim i as integer		  app.showsavetitle = me.value		  		  for i = 0 to windowcount-1		    if window(i) isa  monitor then		      monitor.test.savetitle=me.value		      monitor.test.refresh		    end if		  next		End Sub	#tag EndEvent#tag EndEve