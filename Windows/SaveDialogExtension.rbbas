#tag ModuleProtected Module SaveDialogExtension	#tag Method, Flags = &h0		Sub Disclosure(extends dlg as saveasDialog, assigns v as boolean)		  if v then		    setbinarypreference "AppleNavServices:PutFile:0:Disclosure", "AQ=="		  else		    setbinarypreference "AppleNavServices:PutFile:0:Disclosure", "AA=="		  end		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub ExtensionHidden(extends dlg as saveasDialog, assigns v as boolean)		  if v then		    setbinarypreference "AppleNavServices:PutFile:0:ExtensionHidden", "AQ=="		  else		    setbinarypreference "AppleNavServices:PutFile:0:ExtensionHidden", "AA=="		  end		  		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub SetBinaryPreference(key as string, data as string)		  		  // disabled dependency MBS COSMETIC FEATURE		  #if kMBSused		    dim pref as cfpreferencesMBS		    dim o as cfmutableBinaryDataMBS		    dim m as memoryBlock		    		    pref = new cfpreferencesMBS		    o = new cfmutableBinaryDataMBS		    o.IncreaseLength(lenb(data))		    o.appendstr data		    pref.SetAppValue NewCFStringMBS(key), o, pref.kCFPreferencesCurrentApplication		    		    if not pref.AppSynchronize(pref.kCFPreferencesCurrentApplication) then		      msgbox "synchronize failed"		    end		  #endif		  		  		End Sub	#tag EndMethod	#tag ViewBehavior		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue="-2147483648"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty	#tag EndViewBehaviorEnd Module#tag EndModule