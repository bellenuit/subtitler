True      Width           =   100   End   Begin PushButton Cancelbutton      AutoDeactivate  =   True      Bold            =   False      ButtonStyle     =   0      Cancel          =   False      Caption         =   "Cancel"      Default         =   False      Enabled         =   True      Height          =   20      HelpTag         =   ""      Index           =   -2147483648      InitialParent   =   ""      Italic          =   False      Left            =   79      LockBottom      =   False      LockedInPosition=   False      LockLeft        =   False      LockRight       =   False      LockTop         =   False      Scope           =   0      TabIndex        =   3      TabPanelIndex   =   0      TabStop         =   True      TextFont        =   "System"      TextSize        =   0      TextUnit        =   0      Top             =   73      Underline       =   False      Visible         =   True      Width           =   100   EndEnd#tag EndWindow#tag WindowCode	#tag Event		Sub Open()		  		  wasEnter = false		  		  		  		  		  setlanguage		  		End Sub	#tag EndEvent	#tag Method, Flags = &h0		Sub Ask(mes as string, tit as string)		  message.text = mes		  title = tit		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function GetTimeCode() As timecode		  return timeCodevalue.clone		End Function	#tag EndMethod	#tag Method, Flags = &h0		Sub SetLanguage()		  dim value as string		  dim languagedict as dictionary		  languagedict = app.languagedict		  if languagedict =nil then return		  		  if languagedict.getstring("cancel", value) then cancelbutton.caption= value		  if languagedict.getstring("ok", value) then okbutton.caption = value		  'if languagedict.getstring("savechanges", value) then		  'staticText1.text = replace(value,"^1", app.actmain.title)		  'end		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub SetTimeCode(tc as timecode)		  istimecode = true		  timecodevalue = tc.clone		  edit.text = timeCodevalue.GetText		  edit.selstart = 0		  edit.sellength = len(edit.text)		End Sub	#tag EndMethod	#tag Property, Flags = &h0		ButtonPressed As string	#tag EndProperty	#tag Property, Flags = &h21		Private istimecode As boolean	#tag EndProperty	#tag Property, Flags = &h21		Private timecodevalue As timecode	#tag EndProperty	#tag Property, Flags = &h0		value As double	#tag EndProperty	#tag Property, Flags = &h0		wasEnter As boolean	#tag EndProperty#tag EndWindowCode#tag Events Edit	#tag Event		Sub TextChange()		  if istimecode then		    timeCodevalue.settext me.text		    OKButton.enabled = true		  else		    value = val(me.text)		    if val(me.text)>0 then		      OKButton.enabled = true		    else		      OKButton.enabled = false		    end		  end		End Sub	#tag EndEvent	#tag Event		Function KeyDown(Key As String) As Boolean		  if val(me.text)>0  then		    OKButton.enabled = true		    select case asc(key)		    case 3 'enter		      OKButton.push		    case 13 'cr		      OKButton.push		    end		  end		End Function	#tag EndEvent#tag EndEvents#tag Events OKButton	#tag Event		Sub Open()		  me.enabled = false		End Sub	#tag EndEvent	#tag Event		Sub Action()		  ButtonPressed = me.name		  hide		End Sub	#tag EndEvent#tag EndEvents#tag Events Cancelbutton	#tag Event		Sub Action()		  ButtonPressed = me.name		  hide		End Sub	#tag EndEvent#tag EndEvents